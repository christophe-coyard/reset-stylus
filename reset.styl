* 
	margin 0
	padding 0 
	position relative
	box-sizing border-box
	&:after, &:before
		content ''
		display block
img 
	border none 
	vertical-align middle 
ul 
	list-style none 
a 
	color inherit 
	text-decoration none
body 
	font-family Arial, sans-serif
	color #333
	font-size 16px
	line-height 1
table
	border-collapse collapse
	border-spacing 0
